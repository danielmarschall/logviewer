parameters:
	level: 5
	fileExtensions:
		- php
		- phps
		- sh
	paths:
		- .
	excludePaths:
		analyseAndScan:
			- .phpstan.tmp
	tmpDir: .phpstan.tmp
	bootstrapFiles:
		- config.inc.php
	ignoreErrors:
		- '#is always (true|false)\.#'
		#- '#Call to function assert\(\) with false will always evaluate to false\.#'
		#- '#with no typehint specified\.#'
